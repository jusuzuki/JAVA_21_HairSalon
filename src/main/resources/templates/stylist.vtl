<h3 class="well">List of clients that see: $stylist.getNameStylist()</h3>

<table class="table table-striped">
  <tr>
    <thead>
      <th>Client Name</th>
      <th>-</th>
      <th>-</th>
    </thead>
  </tr>
  <tr>
    <tbody>
      #foreach ($client in $clients)
      <tr>
        <td>$client.getNameClient()</td>
        <td><a href="/clients/$client.getId()/update" class="btn btn-warning btn-xs">Edit</a></td>
        <td><a href="/clients/$client.getId()/delete" class="btn btn-danger btn-xs">Delete</a></td>
      </tr>
      #end
    </tbody>
</table>

<br>
<hr />
<a href="/" class="btn btn-default">Home</a>
